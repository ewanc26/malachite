{
  "name": "lastfm-importer",
  "version": "0.1.0",
  "description": "Import Last.fm scrobbles to ATProto with rate limiting",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "lastfm-import": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "dry-run": "npm run build && node dist/index.js --dry-run",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "lastfm",
    "atproto",
    "bluesky",
    "import",
    "typescript"
  ],
  "author": "",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@atproto/api": "^0.13.35",
    "csv-parse": "^5.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "typescript": "^5.9.3"
  }
}
